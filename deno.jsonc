{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxFragmentFactory": "Fragment",
    "jsxImportSource": "npm:hono@3.12.12/jsx"
  },
  "tasks": {
    "fmt": "deno fmt ./**/*.ts",
    "fmt:check": "deno fmt --check ./**/*.ts",
    "check": "deno check ./**/*.ts",
    "lint": "deno lint ./**/*.ts"
  },
  "lock": false,
  "fmt": {
    "exclude": [
      "CHANGELOG.md"
    ]
  }
}
